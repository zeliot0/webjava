{{ form_start(form) }}


<div class="form-group">
    {{ form_row(form.nom) }}
</div>

<div class="form-group">
    {{ form_row(form.description) }}
</div>

<div class="form-row">
    {{ form_row(form.limite) }}
    {{ form_row(form.type) }}
</div>

<div class="form-checks">
    {{ form_row(form.statut) }}
</div>

<div class="form-actions">
    <button class="btn primary">
        {{ button_label|default('Enregistrer') }}
    </button>
</div>

{{ form_end(form) }}
